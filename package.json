{
  "name": "captionease-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn for installing: https://yarnpkg.com/en/docs/install')\"",
    "start": "NODE_ENV=local STAGE=local ./bin/start.sh",
    "start:development": "NODE_ENV=development STAGE=development ./bin/start.sh",
    "start:ci": "BYPASS_AUTH=true yarn run start",
    "build": "webpack",
    "cleanup": "rm -Rf build && mkdir -p build && rm -Rf .webpack",
    "zip": "yarn cleanup && yarn build && cd .webpack && node ../bin/handlers.js | while read i; do zip ../build/${i}.zip -r ${i}.js; done",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache --fix",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --list-different src/",
    "prettier:write": "prettier --write src/",
    "typecheck": "concurrently --raw yarn:typecheck:*",
    "typecheck:services": "tsc --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand",
    "deploy:notify": "babel-node ./bin/deploy-notify.js cameron",
    "deploy:development": "AWS_PROFILE=captionease-development-admin STAGE=development ./bin/deploy.sh",
    "deploy:central:development": "AWS_PROFILE=captionease-admin STAGE=development ./bin/deploy-core.sh",
    "deploy:production": "AWS_PROFILE=captionease-admin STAGE=production ./bin/deploy.sh",
    "deploy:central:production": "AWS_PROFILE=captionease-admin STAGE=production ./bin/deploy-core.sh"
  },
  "dependencies": {
    "@aws-cdk/aws-ecr": "^1.116.0",
    "@aws-cdk/aws-s3-notifications": "^1.118.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@sentry/serverless": "^6.3.5",
    "@sentry/tracing": "^6.3.5",
    "@supabase/supabase-js": "^1.11.14",
    "aws-cdk": "^1.107.0",
    "aws-lambda": "^1.0.6",
    "babel-plugin-import-graphql": "^2.8.1",
    "babel-plugin-inline-import": "^3.0.0",
    "convict": "^6.1.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "joi": "^17.4.0",
    "slack-webhook": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.51.0",
    "@aws-cdk/aws-apigatewayv2": "^1.102.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.102.0",
    "@aws-cdk/aws-cloudfront": "^1.85.0",
    "@aws-cdk/aws-cognito": "^1.95.1",
    "@aws-cdk/aws-dynamodb": "^1.95.1",
    "@aws-cdk/aws-elasticbeanstalk": "^1.97.0",
    "@aws-cdk/aws-iam": "^1.51.0",
    "@aws-cdk/aws-lambda": "^1.51.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.110.1",
    "@aws-cdk/aws-rds": "^1.51.0",
    "@aws-cdk/aws-s3": "^1.51.0",
    "@aws-cdk/aws-s3-assets": "^1.97.0",
    "@aws-cdk/aws-s3-notifications": "^1.51.0",
    "@aws-cdk/aws-secretsmanager": "^1.51.0",
    "@aws-cdk/aws-sns": "^1.51.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.51.0",
    "@aws-cdk/aws-sqs": "^1.51.0",
    "@aws-cdk/aws-ssm": "^1.51.0",
    "@aws-cdk/core": "^1.66.0",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.2",
    "@types/webpack": "^4.4.27",
    "axios": "^0.24.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0-beta",
    "babel-node": "^0.0.1-security",
    "babel-plugin-const-enum": "^0.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-styled-components": "^1.12.0",
    "dynamodb-toolbox": "^0.3.4",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "tslint": "^5.20.0",
    "typescript": "^4.2.3",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-graphql-loader": "^1.0.2",
    "webpack-node-externals": "^2.5.2"
  }
}
